<body class="backgroundhome">

<div class="form">
<h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, class: 'claseinut'  %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, class: 'claseinut', autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div>
    <%= f.submit "Log in", class: 'actions' %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
<%= link_to "Back Home", home_path %>

</div>

</body>